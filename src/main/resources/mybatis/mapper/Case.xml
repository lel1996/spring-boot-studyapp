<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lierlin.springboo.mapper.CaseMapper">
    <select id="Case" resultType="com.lierlin.springboo.bean.Case">
        select * from `case`
    </select>

    <select id="Count" resultType="int">
        select  count(*) from `case`
    </select>

</mapper>